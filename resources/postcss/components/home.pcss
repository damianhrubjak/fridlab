.heading-shadow {
    text-shadow: 5px 5px 0 var(--pallette-dark-blue),
        10px 10px 0 var(--pallette-blue);
}
.side-shape {
    @apply absolute top-0 h-full w-56 object-cover opacity-50;
}

.cta-button {
    box-shadow: inset -6px -6px #c4be08, 6px 0px 0px 0px #747109,
        0px 6px 0px 0px #7d7905, -6px 0px 0px 0px #7d7905,
        0px -6px 0px 0px #7d7905;
}
